State(false);\r\n\r\n    // Mouse Handler ------------------------------------------\r\n    function handleClick() {\r\n        setSelected(true);\r\n    };\r\n\r\n    // User presses 'enter' key --------------------------------\r\n    useEffect(()=> {\r\n        if (enterSelect) {\r\n            setSelected(true);\r\n        };\r\n    }, [enterSelect]);\r\n\r\n    // INTERACTIVITY /////////////////////////////////////////////////////\r\n    const [interactivity, setInteractivity] = useState(\"able\");\r\n\r\n    useEffect(()=> {\r\n        if (interactivity !== \"dormant\") {\r\n            if (selected) {\r\n                setInteractivity(\"selected\");\r\n            }\r\n            else if (highlight) {\r\n                setInteractivity(\"highlight\");\r\n            }\r\n            else {\r\n                setInteractivity(\"able\");              \r\n            };\r\n        }\r\n    }, [highlight, selected]);\r\n\r\n    // PROPS ////////////////////////////////////////////////////////////\r\n    // Children.forEach(children, (child, index) => {\r\n    //     if (index === 0) {\r\n    //         layers.push(\r\n    //             cloneElement(child, {\r\n    //                 tabIndex: tabIndex,\r\n    //                 enterSelect: enter,\r\n    //                 \"triggerExit\": triggerExit,\r\n    //             })\r\n    //         );\r\n    //     }\r\n    //     else {\r\n    //         layers.push(child);\r\n    //     };\r\n    // });\r\n    // const child = cloneElement(children, {\r\n    //     interactivity: interactivity,\r\n    //     onClick: handleClick,\r\n    //     onMouseEnter: handleMouseEnter,\r\n    //     onMouseLeave: handleMouseLeave,\r\n    // });\r\n\r\n    // console.log(child);\r\n\r\n    const modifiedChildren = Children.map(children, (child, index)=> {\r\n        if (index === 0) {\r\n            return cloneElement(child, {\r\n                interactivity: interactivity,\r\n                onClick: handleClick,\r\n                onMouseEnter: handleMouseEnter,\r\n                onMouseLeave: handleMouseLeave,\r\n            });\r\n        }\r\n        else {\r\n            return cloneElement(child, {\r\n                interactivity: interactivity,\r\n            });\r\n        };\r\n    });\r\n\r\n\r\n    // RENDER //////////////////////////////////////////////////////////////\r\n    return (\r\n        modifiedChildren\r\n    );\r\n};\r\n\r\nexport default InteractiveElement;","C:\\Users\\DoubleD\\Desktop\\Code\\2021\\003 Framework\\src\\App\\Content\\2 Elements\\Links\\Link.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2021\\003 Framework\\src\\App\\Content\\2 Elements\\Links\\LinkTypes\\SiteLink.js",["491"],"// IMPORTS ///////////////////////////////////////////////////////////////\r\n// Import libraries ----------------------------------------------------\r\nimport {useEffect} from \"react\";\r\n\r\nexport default function SiteLink({\r\n    children, interactivity, linkTo, triggerExit,\r\n}) {\r\n    \r\n    useEffect(()=> {\r\n        if (interactivity === \"selected\") {\r\n            triggerExit(linkTo);\r\n        };\r\n    },[interactivity]);\r\n\r\n    // RENDER /////////////////////////////////////////////////////////////////////////////\r\n\r\n    return (\r\n        <>\r\n        </>\r\n    );   \r\n};","C:\\Users\\DoubleD\\Desktop\\Code\\2021\\003 Framework\\src\\App\\Content\\1 Sections\\Page1\\Page1.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2021\\003 Framework\\src\\App\\Content\\1 Sections\\Page1\\Page1Layer1.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2021\\003 Framework\\src\\App\\Content\\1 Sections\\Page1\\Page1Layer0.js",["492"],"C:\\Users\\DoubleD\\Desktop\\Code\\2021\\003 Framework\\src\\App\\Content\\2 Elements\\Links\\LinkStyles\\LinkStyle0.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2021\\003 Framework\\src\\App\\Tools\\Styling Functions\\positioning.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2021\\003 Framework\\src\\App\\Content\\2 Elements\\Images\