\n\r\n    // User presses 'enter' key --------------------------------\r\n    useEffect(()=> {\r\n        if (enterSelect) {\r\n            setSelected(true);\r\n        };\r\n    }, [enterSelect]);\r\n\r\n    // Trigger sound effect and exitting phase for layer -------------------\r\n    useEffect(()=> {\r\n        if (selected) {\r\n            triggerExit(linkTo);\r\n            setPlaySfx(1);\r\n        };\r\n    },[selected]);\r\n\r\n    // INTERACTIVITY /////////////////////////////////////////////////////\r\n    const [interactivity, setInteractivity] = useState(\"able\");\r\n\r\n    useEffect(()=> {\r\n        if (interactivity !== \"dormant\") {\r\n            if (selected) {\r\n                setInteractivity(\"selected\");\r\n            }\r\n            else if (highlight) {\r\n                setInteractivity(\"highlight\");\r\n            }\r\n            else {\r\n                setInteractivity(\"able\");              \r\n            };\r\n        }\r\n    }, [highlight, selected])\r\n\r\n    // RENDER /////////////////////////////////////////////////////////////////////////////\r\n\r\n    return (\r\n        <div>\r\n            <RothkoDiv5\r\n                spatial={[\r\n                    spatial[0] - 4,\r\n                    spatial[1] - 2,\r\n                    spatial[2] + 8,\r\n                    spatial[3] + 2,\r\n                ]}\r\n                width={width}\r\n                interactivity={interactivity}\r\n                colors={[colors[2], colors[0]]}\r\n            />  \r\n            <RothkoDiv1\r\n                colors={colors}\r\n                borderSize={borderSize}\r\n                spatial={spatial}\r\n                width={width}\r\n                interactivity={interactivity}\r\n                blur={200}\r\n            />\r\n            <StyledNavLink \r\n                colors={colors}\r\n                spatial={spatial}\r\n                width={width}\r\n                interactivity={interactivity}\r\n 